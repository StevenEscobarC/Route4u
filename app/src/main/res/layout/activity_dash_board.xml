<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/buttonTipoV"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="Tipo VehÃ­culo"
            android:layout_marginTop="150dp"
            android:layout_gravity="center"
            android:fontFamily="@font/montserratbold"
            android:backgroundTint="@color/btnPrimary"/>
        <Button
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="Tipo Combustible"
            android:layout_gravity="center"
            android:id="@+id/buttonTipoC"
            android:fontFamily="@font/montserratbold"
            android:backgroundTint="@color/btnPrimary"/>
        <Button

            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="Rutas"
            android:layout_gravity="center"
            android:id="@+id/buttonRuta"
            android:fontFamily="@font/montserratbold"
            android:backgroundTint="@color/btnPrimary"/>
        <Button
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="Empresa"
            android:layout_gravity="center"
            android:id="@+id/buttonEmpresa"
            android:fontFamily="@font/montserratbold"
            android:backgroundTint="@color/btnPrimary"/>

        <Button
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:text="Mapa"
            android:layout_gravity="center"
            android:id="@+id/buttonMapa"
            android:fontFamily="@font/montserratbold"
            android:backgroundTint="@color/btnPrimary"/>

        <Button
            android:id="@+id/btnSalir"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Salir"
            android:fontFamily="@font/montserratbold"
            android:backgroundTint="@color/btnPrimary"/>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>